            },
            "0.0.2": {
                "en": "First connection",
                "de": "Erster Verbindungsaufbau möglich"
            },
            "0.0.1": {
                "en": "initial release",
                "de": "Erstveröffentlichung"
            }
        },
        "title": "ComfoAirQ",
        "titleLang": {
            "en": "ComfoAirQ",
            "de": "ComfoAirQ"
        },
        "desc": {
            "en": "Zehnder ComfoAirQ over ComfoConnect LAN C",
            "de": "Zehnder ComfoAirQ über ComfoConnect LAN C"
        },
        "authors": [
            "Matthias Kleine <info@haus-automatisierung.com>"
        ],
        "keywords": [
            "KWL",
            "Zehnder",
            "Lüftung",
